        <Label x:Name="name_Copy13" Content="ClearBackupFolder2" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,399,0,-1"/>
        <TextBox x:Name="CBF2" HorizontalAlignment="Left" Height="23" Margin="160,401,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy14" Content="TaskActive" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,454,0,0"/>
        <TextBox x:Name="TaskActive" HorizontalAlignment="Left" Height="23" Margin="160,456,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy15" Content="TaskStart" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,482,0,0"/>
