        <TextBox x:Name="DType" HorizontalAlignment="Left" Height="23" Margin="160,67,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy2" Content="SQLConnectionString" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,93,0,0"/>
        <TextBox x:Name="SQLCString" HorizontalAlignment="Left" Height="23" Margin="160,95,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy3" Content="SQLDatabaseName" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,122,0,0"/>
        <TextBox x:Name="SQLDName" HorizontalAlignment="Left" Height="23" Margin="160,124,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy4" Content="BackupFolder" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,150,0,0"/>
        <TextBox x:Name="BFolder" HorizontalAlignment="Left" Height="23" Margin="160,152,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy5" Content="RemoteFolder" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,177,0,0"/>
        <TextBox x:Name="RFolder" HorizontalAlignment="Left" Height="23" Margin="160,179,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy6" Content="LocalFolder" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,205,0,0"/>
        <TextBox x:Name="LFolder" HorizontalAlignment="Left" Height="23" Margin="160,207,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy7" Content="BackupFolder1" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,232,0,0"/>
