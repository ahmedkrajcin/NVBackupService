        <TextBox x:Name="TaskRepeat" HorizontalAlignment="Left" Height="23" Margin="160,535,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy18" Content="TaskLast" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,561,0,0"/>
        <TextBox x:Name="TaskLast" HorizontalAlignment="Left" Height="23" Margin="160,563,0,0" TextWrapping="Wrap" IsReadOnly="True" VerticalAlignment="Top" Width="312"/>

        <Button x:Name="saveButton" Content="Save Changes" HorizontalAlignment="Left" Margin="25,592,0,0" VerticalAlignment="Top" Width="109"/>
        <Label x:Name="name_Copy19" Content="Name" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,426,0,0"/>
        <TextBox x:Name="Name" HorizontalAlignment="Left" Height="23" Margin="160,428,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>

    </Grid>
