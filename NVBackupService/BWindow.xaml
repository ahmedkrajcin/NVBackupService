        <Label x:Name="name_Copy8" Content="BackupFolder2" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,260,0,0"/>
        <TextBox x:Name="BF2" HorizontalAlignment="Left" Height="23" Margin="160,262,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy9" Content="ClearBackupFolder" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,287,0,0"/>
        <TextBox x:Name="CBFolder" HorizontalAlignment="Left" Height="23" Margin="160,289,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy10" Content="ClearRemoteFolder" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,315,0,0"/>
        <TextBox x:Name="CRFolder" HorizontalAlignment="Left" Height="23" Margin="160,317,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy11" Content="ClearLocalFolder" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,344,0,0"/>
        <TextBox x:Name="CLFolder" HorizontalAlignment="Left" Height="23" Margin="160,346,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="312"/>
        <Label x:Name="name_Copy12" Content="ClearBackupFolder1" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,372,0,0"/>
